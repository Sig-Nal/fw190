<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>switch-ger.ac</path>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>switch</object-name>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>switch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
				<property>controls/lighting/landing-light</property>
      </binding>
    </action>
  </animation>
  
 <animation>
  <type>rotate</type>
  <object-name>switch</object-name>
				<property>controls/lighting/landing-light</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>60</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.002</x-m>
   <y-m>0.0</y-m>
   <z-m>0.00</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
  
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.25</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>

</PropertyList>

